! built with mkxdefaults

*locale:UTF-8

mupdf*pointerMode:1

URxvt*foreground:white
URxvt*background:black
URxvt*boldFont:xft:Cousine:pixelsize=26:bold
URxvt*boldItalicFont:xft:Cousine:pixelsize=26:bold:italic
URxvt*font:xft:Cousine:pixelsize=26
URxvt*italicFont:xft:Cousine:pixelsize=26:italic
URxvt*color0:#000000
URxvt*color1:#A80000
URxvt*color2:#00A800
URxvt*color3:#A8A800
URxvt*color4:#0000A8
URxvt*color5:#A800A8
URxvt*color6:#00A8A8
URxvt*color7:#A8A8A8
URxvt*color8:#000054
URxvt*color9:#FF0054
URxvt*color10:#00FF54
URxvt*color11:#FFFF54
URxvt*color12:#0000FF
URxvt*color13:#FF00FF
URxvt*color14:#00FFFF
URxvt*color15:#FFFFFF
URxvt*loginShell:true
URxvt*pointerBlank:true
URxvt*saveLines:0
URxvt*scrollBar:false

xterm*background:black
xterm*colorMode:true
xterm*cursorBlink:true
xterm*cursorColor:NavajoWhite
xterm*cursorOffTime:100
xterm*cursorOnTime:1000
xterm*dynamicColors:false
xterm*foreground:white
xterm*logInhibit:true
xterm*loginShell:true
xterm*pointerMode:1
xterm*saveLines:0
xterm*scrollBar:false
xterm*trimSelection:true
xterm*visualBell:true
xterm*visualBellDelay:0

