autogroup 1 "XTerm"
autogroup 2 "ZAngband"
autogroup 3 "cousterm"
autogroup 4 "wrterm"
bind-key C-/ menu-window
bind-key C-F1 lock
color activeborder goldenrod
color inactiveborder grey11
command cous @@HOME@@/libexec/cousterm
command rouge @@HOME@@/libexec/rogueterm
command votcana @@HOME@@/libexec/votcana
command zangband @@HOME@@/libexec/zangband
ignore xbattery
ignore xclipboard
ignore xeyes
ignore xload
wm fvwm /usr/X11R6/bin/fvwm
