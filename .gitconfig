[core]
	excludesfile = ~/.gitignore
	pager = 
	editor = vi
	autocrlf = false
[user]
        name = Jeremy Mates
        email = jmates@example.org
[alias]
	co = checkout
	br = branch
	ci = commit -q
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !gitk
[push]
	default = simple
[color]
	ui = false
