[core]
	excludesfile = ~/.gitignore
	pager = 
	editor = vi
	autocrlf = false
[user]
        name = Jeremy Mates
        email = jmates@example.org
[alias]
	br = branch
	ci = commit --quiet
	co = checkout
	last = log --patch -1 HEAD
	rbi = rebase --interactive
	st = status
	unstage = reset HEAD --
[push]
	default = simple
[color]
	ui = false
